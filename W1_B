#include <iostream>
#include <stack>
#include <algorithm>
#include <vector>

using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);

	int t;
	int q;

	cin >> t;

	for (int i = 0; i < t; i++) {
		int n;
		int x;
		int small = 1001;
		int big = 0;

		vector<int> vec;

		cin >> n;

		for (int j = 0; j < n; j++) {
			cin >> q;

			if (q == 1) {//삽입
				cin >> x;

				vec.push_back(x);

				if (x < small) {
					small = x;
				}

				if (x > big) {
					big = x;
				}
				
				cout << small << " " << big << "\n";
			}
			else {//삭제
				if (!vec.empty()) {
					int tmp = vec.back();
					vec.pop_back();

					if (tmp == small) {
						small = 1001;

						for (int i = 0; i < vec.size(); i++) {
							if (vec[i] < small) {
								small = vec[i];
							}
						}
					}

					if (tmp == big) {
						big = 0;
				
						for (int i = 0; i < vec.size(); i++) {
							if (vec[i] > big) {
								big = vec[i];
							}
						}
					}
				}
			}
		}
	}

	return 0;
}
